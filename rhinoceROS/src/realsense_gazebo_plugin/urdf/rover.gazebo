<link name="base_lidar_front">

        <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>

    <collision name='collision'>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
	<origin xyz="0 0 0" rpy="0 0 0"/>
   	<material name="red"/>
    </collision>

    <visual>

      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
	<origin xyz="0 0 0" rpy="0 0 0"/>
	<material name="red"/>

    </visual>
</link>

<gazebo reference="base_lidar_front">
